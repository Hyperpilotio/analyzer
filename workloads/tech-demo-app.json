{
  "name": "tech-demo",
  "microservices": [
    {
      "name": "goddd",
      "service_id": "%SERVICE_ID1%",
      "status": "Running"
    },
    {
      "name": "mongo-serve",
      "service_id": "%SERVICE_ID2%",
      "status": "Pending"
    },
    {
      "name": "pathfinder",
      "service_id": "%SERVICE_ID3%",
      "status": "Running"
    }
  ],
  "type": "long-running",
  "slo": {
    "metric": "hyperpilot/goddd/api_booking_service_request_latency_milliseconds",
    "type": "latency",
    "summary": "quantile_90",
    "value": 300,
    "unit": "milliseconds"
  },
  "management_features": [
    {
      "name": "interference_management",
      "mode": "Enabled",
      "remediation_policy": []
    },
    {
      "name": "bottleneck_management",
      "mode": "Enabled",
      "remediation_policy": []
    },
    {
      "name": "efficiency_management",
      "mode": "Disabled"
    }
  ]
}
