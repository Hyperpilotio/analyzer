{
  "name": "tech-demo",
  "microservices": [
    {
      "name": "goddd",
      "service_id": "service-0001",
      "namespace": "default",
      "kind": "Deployment"
    },
    {
      "name": "mongo",
      "service_id": "service-0002",
      "namespace": "default",
      "kind": "Deployment"
    },
    {
      "name": "pathfinder",
      "service_id": "service-0003",
      "namespace": "default",
      "kind": "Deployment"
    }
  ],
  "type": "long-running",
  "slo": {
    "source": {
        "APM_type": "prometheus",
        "service_name": "goddd",
        "port": 9090
    },
    "metric": "hyperpilot/goddd/api_booking_service_request_latency_milliseconds",
    "type": "latency",
    "summary": "quantile_90",
    "value": 300,
    "unit": "milliseconds"
  },
  "management_features": [
    {
      "name": "interference_management",
      "status": "Enabled",
      "remediation_policy": []
    },
    {
      "name": "bottleneck_management",
      "status": "Enabled",
      "remediation_policy": []
    },
    {
      "name": "efficiency_management",
      "status": "Disabled"
    }
  ]
}
